{
  "rewrites": [
    {
      "source": "/__/auth/:path*",
      "destination": "https://snipshift-75b04.firebaseapp.com/__/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://api.hospogo.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}