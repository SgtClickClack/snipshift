{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/sounds/:path*", "destination": "/sounds/:path*" },
    { "source": "/api/:path*", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index" }
  ],
  "cleanUrls": true
}
